<%= render "layouts/errors", :object =>@foods %>

<%= form_for @food do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :area %>
  <%= f.text_field :area %>
  <%= f.submit %>
<% end %>
